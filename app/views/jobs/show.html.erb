<p id="notice"><%= notice %></p>

<h1>Show Jobs</h1>



<table class="table table-striped col-md-4 col-md-offset-1">
  <tr>
    <td class="col-xs-6 col-sm-3 "><strong>Title:</strong></td>
    <td><%= @job.title %></td>
  </tr>
   <tr>
    <td><strong>Job type:</strong></td>
    <td><%= @job.job_type %></td>
  </tr>
  <tr>
    <td><strong>Company name:</strong></td>
    <td><%= @job.company_name %></td>
  </tr>
    <tr>
    <td><strong>Company Web:</strong></td>
    <td><%= @job.url %></td>
  </tr>
  <tr>
    <td><strong>Occupation:</strong></td>
    <td><%= @job.occupation %></td>
  </tr>
  <tr>
    <td><strong>Location:</strong></td>
    <td><%= @job.location %></td>
  </tr>
  <tr>
    <td><strong>Description:</strong></td>
    <td><%= @job.description %></td>
  </tr>
  <tr>
    <td><strong>Apply information:</strong></td>
    <td><%= @job.apply_information %></td>
  </tr>
  <tr>
    <td><strong>Deadline:</strong></td>
    <td><%= @job.deadline %></td>
  </tr>
</table>



<%= link_to 'Edit', edit_job_path(@job) ,class:'btn btn-info btn-sm' %> 
<%= link_to 'Back', jobs_path, class:'btn btn-info btn-sm' %>
